{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "prisma generate && babel src -d dist && npm run build "
      }
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/emails/(.*)\\.jsx",
      "dest": "/emails/$1"
    }
  ]
}
